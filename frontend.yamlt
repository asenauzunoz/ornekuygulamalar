apiVersion: apps/v1
kind: Deployment
metadata:
  name: frontend-deployment
spec:
  replicas: 3
  selector:
    matchLabels:
      app: frontend
  template:
    metadata:
      labels:
        app: frontend
    spec:
      containers:
      - name: frontend
        image: kullanici_adi/frontend:latest  # Docker imajının adını ve etiketini kendi bilgilerinizle değiştirin
        ports:
        - containerPort: 80

